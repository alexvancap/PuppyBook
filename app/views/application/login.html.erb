<h1>login</h1>

<%=form_with method: 'post', url: '/login' do |form|%>
    
    <label>email:</label>
        <%=flash[:email]%>
    <%=form.text_field(:email)%>

    <label>password:</label>
        <%=flash[:password]%>
    <%=form.password_field(:password)%>

    <%=form.submit('login')%>
<%end%>

